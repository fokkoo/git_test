<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".HistoryFragment">



    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/recyclerViewHistory2"
        android:layout_width="match_parent"
        android:layout_height="97dp"
        app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager" />



    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <TextView
            android:id="@+id/textView4"
            android:layout_width="221dp"
            android:layout_height="39dp"
            android:text=" " />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="221dp"
            android:layout_height="39dp"
            android:text=" Fragment history" />

        <TextView
            android:id="@+id/textViewMySQL"
            android:layout_width="399dp"
            android:layout_height="41dp"
            android:text="export_text"
            android:textColor="@color/teal_200"
            />


        <EditText
            android:id="@+id/editTextNumber"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:ems="100000"
            android:inputType="number"
            android:textColor="@color/teal_200"
            />

        <TextView
            android:id="@+id/textViewFromEditText"
            android:layout_width="399dp"
            android:layout_height="41dp"
            android:text="export_text"
            android:textColor="@color/teal_200"
            />

        <Button
            android:id="@+id/button_start"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Button Start"
            />
        <TextView
            android:id="@+id/textView8"
            android:layout_width="221dp"
            android:layout_height="39dp"
            android:text=" id temperature city timestamp" />
        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/recyclerViewHistory"
            android:layout_width="match_parent"
            android:layout_height="9dp"
            app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager" />
        <Button
            android:id="@+id/button_export_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Button export_text"
            />

        <com.github.mikephil.charting.charts.BarChart
            android:id="@+id/barChart"
            android:layout_width="match_parent"
            android:layout_height="122dp" />

        <TextView
            android:id="@+id/textView24"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="TextView" />


        <!-- TODO: Update blank fragment layout -->



    </LinearLayout>

</FrameLayout>