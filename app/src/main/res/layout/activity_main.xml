<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">
    <FrameLayout
        android:id="@+id/fragment_container"
        android:layout_width="match_parent"
        android:layout_height="600dp"
        app:layout_constraintBottom_toBottomOf="parent"
        tools:layout_editor_absoluteX="9dp">

    </FrameLayout>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Hello GIT!"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Text for 3  commit"
        tools:layout_editor_absoluteX="217dp"
        tools:layout_editor_absoluteY="460dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="first branch started"
        tools:layout_editor_absoluteX="83dp"
        tools:layout_editor_absoluteY="421dp" />

    <Button
        android:id="@+id/Button_History"
        android:layout_width="71dp"
        android:layout_height="59dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="72dp"
        android:text="Button_History"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button_fragment2"
        android:layout_width="204dp"
        android:layout_height="95dp"
        android:text="Button_2_fragment"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button_recycl_view"
        android:layout_width="87dp"
        android:layout_height="93dp"
        android:layout_marginEnd="36dp"
        android:text="Button for RW"
        app:layout_constraintEnd_toStartOf="@+id/button_fragment2"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>